{
    "repositories": {
        "amt-feed-importer": {
            "type": "git",
            "url": "https://github.com/slub/amt_feed_importer.git"
        },
        "kitodo-presentation": {
            "type": "git",
            "url": "https://github.com/kitodo/kitodo-presentation.git"
        },
        "slub-digitalcollections": {
            "type": "git",
            "url": "https://github.com/slub/slub_digitalcollections.git"
        },
        "slub-web-sachsendigital": {
            "type": "git",
            "url": "https://github.com/slub/slub_web_sachsendigital.git"
        },
        "0": {
            "type": "composer",
            "url": "https://composer.typo3.org/"
        }
    },
    "name": "slub/ddev-sachsendigital",
    "description": "DDEV Development Repo for Sachsen.Digital",
    "license": "GPL-3.0",
    "config": {
        "platform": {
            "php": "8.2"
        },
        "allow-plugins": {
            "typo3/cms-composer-installers": true,
            "typo3/class-alias-loader": true,
            "helhum/typo3-console-plugin": true
        }
    },
    "require": {
        "helhum/typo3-console": "^8.1",
        "typo3/minimal": "^11.5",
        "typo3/cms-about": "^11.5",
        "typo3/cms-adminpanel": "^11.5",
        "typo3/cms-belog": "^11.5",
        "typo3/cms-beuser": "^11.5",
        "typo3/cms-composer-installers": "^4.0@rc || >=5.0",
        "typo3/cms-felogin": "^11.5",
        "typo3/cms-fluid-styled-content": "^11.5",
        "typo3/cms-form": "^11.5",
        "typo3/cms-impexp": "^11.5",
        "typo3/cms-info": "^11.5",
        "typo3/cms-lowlevel": "^11.5",
        "typo3/cms-redirects": "^11.5",
        "typo3/cms-reports": "^11.5",
        "typo3/cms-rte-ckeditor": "^11.5",
        "typo3/cms-setup": "^11.5",
        "typo3/cms-seo": "^11.5",
        "typo3/cms-sys-note": "^11.5",
        "typo3/cms-t3editor": "^11.5",
        "typo3/cms-tstemplate": "^11.5",
        "typo3/cms-viewpage": "^11.5",
        "kitodo/presentation": "dev-master",
        "slub/slub-digitalcollections": "dev-master",
        "slub/slub-web-sachsendigital": "dev-main",
        "sgalinski/lfeditor": "^7.2",
        "georgringer/news": "^9.4",
        "typo3/cms-scheduler": "^11.5",
        "typo3/cms-recycler": "^11.5",
        "amt/amt-feed-importer": "^2.0"
    },
    "scripts": {
        "typo3-cms-scripts": [
            "typo3 install:fixfolderstructure"
        ],
        "post-autoload-dump": [
            "@typo3-cms-scripts"
        ]
    }
}
